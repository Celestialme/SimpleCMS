		"mime-types": "^2.1.35",
		"mongoose": "^7.4.0",
		"nodemailer": "^6.9.4",
		"pdf-thumbnail": "^1.0.6",
		"svelte-email": "^0.0.4",
		"svelte-fsm": "^1.2.0",
		"svelte-lexical": "0.3.4",
		"typesafe-i18n": "^5.25.1"
	}
